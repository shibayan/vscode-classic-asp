{
  "name": "classic-asp",
  "displayName": "Classic ASP",
  "description": "Classic ASP Language Support",
  "version": "0.0.2",
  "publisher": "ilich8086",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ilich/vscode-classic-asp/issues"
  },
  "homepage": "https://github.com/ilich/vscode-classic-asp/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilich/vscode-classic-asp"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "asp",
        "aliases": [ "ASP", "asp" ],
        "extensions": [ ".asp", ".asa" ],
        "configuration": "./asp.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "asp",
        "scopeName": "text.html.asp",
        "path": "./syntaxes/asp.json"
      }
    ]
  }
}